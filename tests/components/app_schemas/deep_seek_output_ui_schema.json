{
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "enum": [
        "OpenAI Compatible Chat API",
        "OpenAI Compatible Embeddings API"
      ]
    },
    "Hostname": {
      "type": "string",
      "minLength": 5
    },
    "Port": {
      "type": "string",
      "pattern": "^[0-9]+$"
    },
    "Protocol": {
      "type": "string",
      "enum": ["http", "https"]
    },
    "Connection Timeout": {
      "type": "string",
      "pattern": "^[0-9]+$"
    },
    "Base Path": {
      "type": "string"
    },
    "Api Type": {
      "type": "string",
      "enum": ["rest"]
    },
    "Api Base Path": {
      "type": "string",
      "pattern": "^/"
    },
    "Openai Api Type": {
      "type": "string",
      "enum": ["chat", "embeddings"]
    },
    "Endpoint Url": {
      "type": "string",
      "pattern": "^/v1/"
    },
    "Hugging Face Model Name": {
      "type": "string"
    },
    "Secret": {
      "type": "string"
    }
  },
  "required": [
    "title",
    "Hostname",
    "Port",
    "Protocol",
    "Connection Timeout",
    "Base Path",
    "Api Type",
    "Api Base Path",
    "Openai Api Type",
    "Endpoint Url",
    "Hugging Face Model Name",
    "Secret"
  ],
  "additionalProperties": false
}
